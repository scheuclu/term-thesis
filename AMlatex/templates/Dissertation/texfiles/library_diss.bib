@article{zander_non-smooth_2008,
	title = {Non-smooth dynamics in academia and industry: recent work at {TU} München},
	volume = {195},
	issn = {0001-5970, 1619-6937},
	url = {http://link.springer.com/article/10.1007/s00707-007-0570-5},
	doi = {10.1007/s00707-007-0570-5},
	shorttitle = {Non-smooth dynamics in academia and industry},
	abstract = {The formulation and simulation of multi-body systems with non-smooth dynamical behavior is discussed. Starting with the mathematical framework, bi- and unilateral contacts including friction are introduced. A general concept for the geometrical description of contacting surfaces is presented. Applying the numerical methods developed for non-smooth systems to different examples in academia and industry, the overall framework proves to be robust and efficient.},
	pages = {167--183},
	number = {1},
