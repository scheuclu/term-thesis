	langid = {english},
	keywords = {continuously variable transmission belt, friction loss, lubricants, transmission efficiency, transmittable torque capacity},
	file = {Narita and Priest - 2007 - Metal-metal friction characteristics and the trans.pdf:/home/local_grundl/common/Forschung/Projekte/CVT/30_Grundl/Literatur_Zotero/storage/SX3MUQFG/Narita and Priest - 2007 - Metal-metal friction characteristics and the trans.pdf:application/pdf;Snapshot:/home/local_grundl/common/Forschung/Projekte/CVT/30_Grundl/Literatur_Zotero/storage/XTF6XAJW/11.html:text/html}
