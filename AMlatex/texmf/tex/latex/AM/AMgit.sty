\NeedsTeXFormat{LaTeX2e}
\ProvidesPackage{AMgit}[2015/11/15 v2.0 AM git info]
\def\AM@git@branch{master}
\def\AM@git@branch@template{--branch--}
\def\AM@git@branch@default{master}
\def\AM@git@version{v2.2-8-ga6fff9e}
\def\AM@git@version@template{--version--}
\def\AM@git@version@default{v2.0}
\def\AM@git@date{2016/06/15}
\def\AM@git@date@template{--date--}
\def\AM@git@date@default{2015/01/11}
\newcommand{\AMinsertGitBranch}{%
  \ifx\AM@git@branch\AM@git@branch@template%
    \AM@git@branch@default%
  \else%
    \AM@git@branch%
  \fi%
}
\newcommand{\AMinsertGitVersion}{%
  \ifx\AM@git@version\AM@git@version@template%
    \AM@git@version@default%
  \else%
    \AM@git@version%
  \fi%
}
\newcommand{\AMinsertGitDate}{%
  \ifx\AM@git@date\AM@git@date@template%
    \AM@git@date@default
  \else%
    \AM@git@date%
  \fi%
}
