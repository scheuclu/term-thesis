\RequirePackage{kvoptions}
\newif\ifAM@lang@english
\AM@lang@englishtrue
\newif\ifAM@lang@ngerman
\AM@lang@ngermanfalse
\DeclareOption{english}{\AM@lang@englishtrue\AM@lang@ngermanfalse}
\DeclareOption{ngerman}{\AM@lang@ngermantrue\AM@lang@englishfalse}
\DeclareOption*{\PassOptionsToPackage{\CurrentOption}{babel}}
\ProcessOptions*\relax
\ifAM@lang@ngerman
  \PassOptionsToPackage{english,main=ngerman}{babel}
  \AfterEndPreamble{\selectlanguage{ngerman}}
\else
  \PassOptionsToPackage{english}{babel}
\fi
\RequirePackage{babel}
\newcommand{\AMlangDemo}[1]{%
\foreignlanguage{english}{\textcolor{TUMIvory}{\ensuremath{\blacktriangleright}}\xspace#1}
