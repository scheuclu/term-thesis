% \subsubsection{Scriptum}
%
% \begin{docCommand}{AMtitlepage@scriptum}{}
% \end{docCommand}
%
%    \begin{macrocode}
\newcommand*{\AMtitlepage@scriptum}{
{%
\phantomsection%
\pdfbookmark[1]{Titlepage}{titlepage}%
\setcounter{page}{1}%
\includeHeaderAFour%
\par%
\vspace{1em}
{%
  \fontsize{14pt}{18pt}\selectfont    
  \textit{\AM@subject}\par
}%
  {%
    \color{TUMBlue}\fontsize{22pt}{28pt}\selectfont
    {\bfseries\@title\par}%
	\ifx\AM@subtitle\@empty \else%
		{\itshape\AM@subtitle}\par%
	\fi%
    \medskip%
  }%
  {%
    \fontsize{11pt}{15pt}\selectfont
    \@date\par
  }%  
  {%
    \fontsize{14pt}{18pt}\selectfont
    \@author\par
  }%
}
\vspace{1em}%
\thispagestyle{empty}%
}
%    \end{macrocode}
%
% \subsubsection{Exercise}
%
% \begin{docCommand}{AMtitlepage@exercise}{}
% \end{docCommand}
%
%    \begin{macrocode}
\newcommand*{\AMtitlepage@exercise}{
{%
\phantomsection%
\pdfbookmark[1]{Titlepage}{titlepage}%
\setcounter{page}{1}%
\includeHeaderAFour\par\vspace{4em}
  {%
    \fontsize{22pt}{28pt}\selectfont
    {\bfseries\@title\par}%
\ifx\AM@subtitle\@empty \else%
{\fontsize{14pt}{18pt}\AM@subtitle}\par%
\fi%
    \medskip%
  }%
  {%
    \fontsize{11pt}{15pt}\selectfont
    \@date\par
  }%
  {%
    \fontsize{14pt}{18pt}\selectfont
    \@author\par
  }%
}
\vspace{1em}%
\thispagestyle{empty}%
}
%    \end{macrocode}
