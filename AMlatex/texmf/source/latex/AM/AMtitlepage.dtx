% \subsection{Standard TUM-AM}
%
%
% \begin{docCommand}{AMtitlepage@TUMAM}{}
% \end{docCommand}
%
%    \begin{macrocode}
\newcommand{\AMtitlepage@TUMAM}{%
  \thispagestyle{empty}
  \def\AM@title@style{\bfseries\huge}
  \def\AM@subtitle@style{\bfseries\Large\color{TUMBlue}}
  \date{}
  \pdfbookmark{\@title}{AMtitleanchor}
  \vspace*{-15mm}
  \noindent
  \includeHeaderAFour
  \vskip15mm
  \begin{center}
    \raggedright{\AM@title@style\@title} \\[3ex]
    \ifx\AM@subtitle\empty\else 
      {\AM@subtitle@style\AM@subtitle} \\[2ex]
    \fi
    {\@author \ifx\@date\empty\else-- \@date\fi}
  \end{center}
  \vskip5mm
}
%    \end{macrocode}
