\newcommand*{\AM@supervisor}{}
\newcommand{\supervisor}[1]{\gdef\AM@supervisor{\def\and{\item}#1}}
\newcounter{supervisorCounter}
\newenvironment{supervisorListDiss}{%
  \begin{list}{\arabic{supervisorCounter}.~~}%
    {%
      \usecounter{supervisorCounter}
      \labelsep=0em \labelwidth=0em \leftmargin=0em
      \itemindent=0em \topsep=0em
    }%
  }{%
    \end{list}%
}
\newcommand{\AMtitlepage@print@background}{%
  {\raggedright\vspace*{-\headsep}\hspace*{-1in}\hspace*{-\oddsidemargin}%
    \begin{picture}(0,0)(0,0)%
      \setlength\fboxsep{0pt}%
      \put(0,0){\parbox[t][\textheight][c]{\textwidth}%
      {\includegraphics{AMtitlepage-image-Master}}}
    \end{picture}
  }\par
}
\newcommand{\AMtitlepage@print@author}{%
  \raggedright\sffamily\LARGE\@author\par
}
\newcommand{\AMtitlepage@print@title}{%
  \textcolor{TUMBlue}{\huge\bfseries\@title}\par
}
\newcommand{\AMtitlepage@print@subtitle}{%
  \ifx\AM@subtitle\@empty \else
    \textcolor{TUMBlue2}{\LARGE\AM@subtitle}\par
  \fi
}
\newcommand{\AMtitlepage@print@date}{%
  {\Large \@date \par}
}
\newcommand{\AMtitlepage@print@supervisor}{%
  \ifx\AM@supervisor\@empty \else
    {\large \supervisorstring:\\
    \begin{list}%
      {}%
      {\labelsep=0em \labelwidth=0em \leftmargin=0em \itemindent=0em \topsep=0em}%
      \item\ifx\AM@supervisor\@empty\textbf{?}\else\AM@supervisor\fi%
    \end{list}
    }%
  \fi
}
\newcommand{\AMtitlepage@print@dedication}{%
}
\providecommand\AMtitlepage@arbeittype{}
\newcommand{\AMtitlepage@print@arbeit}{%
  \large\AMtitlepage@arbeittype\par
}
\newcommand{\AMtitlepage@create@titlepage}{%
  \thispagestyle{empty}
  \begin{titlepage}%
    \AMtitlepage@print@background
    \vspace{\stretch{4}}
    \AMtitlepage@print@author
    \vspace{\stretch{1}}
    \AMtitlepage@print@title
    \AMtitlepage@print@subtitle
    \vspace{\stretch{1}}
    \AMtitlepage@print@arbeit
    \vspace{\stretch{3}}
    \AMtitlepage@print@date
    \vspace{\stretch{6}}
    \AMtitlepage@print@supervisor
    \null
  \end{titlepage}
}
\providecommand{\includeHeaderAFour}{%
  \begin{minipage}[t][10mm][t]{\textwidth}%
    \color{TUMBlue}
    \AM@font@load@tumhelvetica
    \fontencoding{T1}\fontseries{m}\fontfamily{lhv}\fontsize{11pt}{11pt}
    \selectfont%
    \noindent%
    \includegraphics[height=11.67mm,width=10.67mm]{AM-logo-AM-blau-RGB}
    \hspace{1ex}%
    \begin{minipage}[b]{0.38\textwidth}
      \raggedright \AMinsertAM
    \end{minipage}%
    \hfill{}%
    \begin{minipage}[b]{0.37\textwidth}
      \raggedleft \AMinsertTUM
    \end{minipage}%
    \hspace{1ex}%
    \includegraphics[height=10mm,width=18.7mm]{AM-logo-TUM-voll-blau-RGB}%
\end{minipage}
\hfill{}%
}
\newcommand{\AMtitlepage@TUMAM}{%
  \thispagestyle{empty}
  \def\AM@title@style{\bfseries\huge}
  \def\AM@subtitle@style{\bfseries\Large\color{TUMBlue}}
  \date{}
  \pdfbookmark{\@title}{AMtitleanchor}
  \vspace*{-15mm}
  \noindent
  \includeHeaderAFour
  \vskip15mm
  \begin{center}
    \raggedright{\AM@title@style\@title} \\[3ex]
    \ifx\AM@subtitle\empty\else
      {\AM@subtitle@style\AM@subtitle} \\[2ex]
    \fi
    {\@author \ifx\@date\empty\else-- \@date\fi}
  \end{center}
  \vskip5mm
}
\AMsetFont{contact info=\color{TUMDarkGray}}
\newcommand{\AMtitlepage@print@contactinfos}{%
  \parbox[c]{10cm}{\small
  \begin{tabular}{rl}
  \AMfont@contactinfo@style Date & \ifx\@date\empty\today\else\@date\fi \\
  \AMfont@contactinfo@style Contact & \@author \\
  \AMfont@contactinfo@style Address & Lehrstuhl f\"{u}r Angewandte Mechanik \\
  \AMfont@contactinfo@style         & Boltzmannstr. 15 \\
                                    & D - 85748 Garching \\
  \AMfont@contactinfo@style Telephone & \AM@contactphone \\
  \AMfont@contactinfo@style E-mail & \AM@contactemail
  \end{tabular}
  }
}
\newcommand{\AMtitlepage@contact}{
  \noindent\hskip-1.5cm
  \AMtitlepage@print@contactinfos
\hfill
\parbox[c]{7cm}{\flushright
\includegraphics{AM-logo-TUM-voll-blau-RGB} \\[1ex]
\large\color{TUMDarkBlue} Technische Universit\"{a}t M\"{u}nchen
\vskip3ex
}
\vskip1ex\noindent
\rule{\textwidth}{0.4pt}
\vskip1ex\noindent
\hfill{\AMfont@title@style\@title}\hfill\null \\[1ex]
\null\hfill{\AMfont@author@style\AM@subtitle}\hfill\null

\noindent\rule{\textwidth}{0.4pt}\vskip3ex
}
\newcommand*{\AMtitlepage@scriptum}{
{%
\phantomsection%
\pdfbookmark[1]{Titlepage}{titlepage}%
\setcounter{page}{1}%
\includeHeaderAFour%
\par%
\vspace{1em}
{%
  \fontsize{14pt}{18pt}\selectfont
  \textit{\AM@subject}\par
}%
  {%
    \color{TUMBlue}\fontsize{22pt}{28pt}\selectfont
    {\bfseries\@title\par}%
\ifx\AM@subtitle\@empty \else%
{\itshape\AM@subtitle}\par%
\fi%
    \medskip%
  }%
  {%
    \fontsize{11pt}{15pt}\selectfont
    \@date\par
  }%
  {%
    \fontsize{14pt}{18pt}\selectfont
    \@author\par
  }%
}
\vspace{1em}%
\thispagestyle{empty}%
}
\newcommand*{\AMtitlepage@exercise}{
{%
